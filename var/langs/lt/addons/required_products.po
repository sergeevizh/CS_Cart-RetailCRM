msgctxt "Addons::name::required_products"
msgid "Required products"
msgstr "Būtini produkai"

msgctxt "Addons::description::required_products"
msgid "Makes it possible to specify products required for purchase"
msgstr "Suteikia galimybę nurodyti produktus, reikalingus įsigyti"

msgctxt "Languages::required_products"
msgid "Required products"
msgstr "Būtini produktai"

msgctxt "Languages::required_products_added"
msgid "The following required product(s) were added to your cart"
msgstr "Reikia šių prekių kad būtų įtraukta į krepšelį"

msgctxt "Languages::required_products_out_of_stock"
msgid "This product cannot be added to your cart, because following required product(s) are out of stock:"
msgstr "Šis produktas negali būti įtrauktas į jūsų krepšelį, nes nėra būtinojo atsargose:"

