msgctxt "Addons::name::access_restrictions"
msgid "Access restrictions"
msgstr "Obmedzenia prístupu"

msgctxt "Addons::description::access_restrictions"
msgid "Lets you limit storefront and administrator area access to certain IP-addresses with different options"
msgstr "Umožňuje obmedziť prístup na stránku obchodu aj do administrácie pre určité IP adresy s rôznymi možnosťami"

msgctxt "Languages::access_restrictions"
msgid "Access restrictions"
msgstr "Obmedzenia prístupu"

msgctxt "Languages::add_ips"
msgid "Add IPs"
msgstr "Pridať IP adresy"

msgctxt "Languages::new_ips"
msgid "New IPs"
msgstr "Nová IP adresa"

msgctxt "Languages::add_domains"
msgid "Add domains"
msgstr "Pridať domény"

msgctxt "Languages::new_domains"
msgid "New domains"
msgstr "Nové domény"

msgctxt "Languages::add_emails"
msgid "Add e-mail addresses"
msgstr "Pridajte e-mailové adresy"

msgctxt "Languages::new_emails"
msgid "New e-mail addresses"
msgstr "Nové e-mailové adresy"

msgctxt "Languages::new_credit_card"
msgid "New credit card"
msgstr "Nová kreditná karta"

msgctxt "Languages::add_credit_card"
msgid "Add credit card"
msgstr "Pridať kreditnú kartu"

msgctxt "Languages::privileges.manage_access_restrictions"
msgid "Manage access restrictions"
msgstr "Spravovať obmedzenia prístupu"

msgctxt "Languages::domain"
msgid "Domain"
msgstr "Doména"

msgctxt "Languages::warning_of_ip_adding"
msgid "The entered IP address <b>[entered_ip]</b> forbids access to the administration panel from your computer (your IP is <b>[your_ip]</b>)."
msgstr "Zadaná adresa IP <b>[entered_ip]</b> zakazuje prístup do administračného panelu z vášho počítača (vaša IP je <b>[your_ip]</b>)."

msgctxt "Languages::text_cc_number_is_blocked"
msgid "The credit card number <b>[cc_number]</b> is blocked by the administrator. Please contact the store administrator or try another one."
msgstr "Číslo kreditnej karty <b>[cc_number]</b> je blokované správcom. Prosím obráťte sa na správcu obchodu alebo skúste inú kartu."

msgctxt "Languages::text_not_valid_ip"
msgid "<b>[ip]</b> is not a valid ip address."
msgstr "<b>[ip]</b> nie je platnou IP adresou."

msgctxt "Languages::text_not_valid_domain"
msgid "<b>[domain]</b> is not a valid domain name."
msgstr "<b>[domain]</b> nie je platným názvom domény."

msgctxt "Languages::text_not_valid_cc_number"
msgid "<b>[cc_number]</b> is not a valid credit card number."
msgstr "<b>[cc_number]</b> nie je číslo platnej kreditnej karty."

msgctxt "Languages::text_email_is_blocked"
msgid "The e-mail address <b>[email]</b> is blocked by the administrator. Please contact the store administrator or try another one."
msgstr "E-mailová adresa <b>[email]</b> je blokovaná správcom. Prosím obráťte sa na správcu obchodu alebo skúste inú emailovú adresu."

msgctxt "Languages::text_ips_denied"
msgid "You are not allowed to access this area"
msgstr "Nemáte povolenie na prístup k tejto oblasti"

msgctxt "Languages::text_ip_blocked_failed_login"
msgid "The IP was blocked after [number] unsuccessful logins"
msgstr "IP adresa bola blokovaná po [number] neúspešných prihláseniach"

msgctxt "Languages::text_ip_is_blocked"
msgid "Your IP address is blocked."
msgstr "Vaša IP adresa je blokovaná."

msgctxt "Languages::store_admin"
msgid "Store administrator"
msgstr "Správca obchodu"

msgctxt "Languages::your_ip_added"
msgid "Your IP address <b>[ip]</b> has been added to the access restrictions table."
msgstr "Vaša IP adresa <b>[ip]</b> bola pridaná do tabuľky obmedzených prístupov."

msgctxt "Languages::your_ip_enabled"
msgid "Your IP address <b>[ip]</b> has been enabled in the access restrictions table."
msgstr "Vaša IP adresa <b>[ip]</b> bola povolená v tabuľke obmedzených prístupov."

msgctxt "Languages::your_ip_removed"
msgid "Your IP address <b>[ip]</b> has been removed from the access restrictions table."
msgstr "Vaša IP adresa <b>[ip]</b> bol odstránená z tabuľky obmedzených prístupov."

msgctxt "Languages::ip_from"
msgid "IP from"
msgstr "IP od"

msgctxt "Languages::ip_to"
msgid "IP to"
msgstr "IP do"

msgctxt "Languages::text_access_notice"
msgid "You can use the wildcards for the \"Domain\", \"E-mail\" and \"Credit Card\" tabs: <br /><b>*</b> - can represent any number of characters, including zero<br /><b>?</b> - can represent any single character."
msgstr "Môžete použiť zástupné znaky pre \"Doména\", \"E-mail\" a \"Kreditná karta\" : < br / ><b>*</b> - môže predstavovať ľubovoľný počet znakov vrátane nuly < br / ><b>?</b> - môže predstavovať ľubovoľný jeden znak."

msgctxt "Languages::ips"
msgid "IPs"
msgstr "IP adresy"

msgctxt "Languages::make_permanent"
msgid "Make permanent"
msgstr "Trvalé"

msgctxt "Languages::credit_card_number"
msgid "Credit card number"
msgstr "Číslo kreditnej karty"

msgctxt "SettingsSections::access_restrictions::general"
msgid "General"
msgstr "Všeobecné"

msgctxt "SettingsOptions::access_restrictions::elm_administrator_area_settings"
msgid "Administrator area settings"
msgstr "Nastavenie administrácie"

msgctxt "SettingsOptions::access_restrictions::admin_reverse_ip_access"
msgid "Allow login to the admin area from specified IPs only"
msgstr "Povoliť prihlásenie do administrácie iba zo zadanej IP adresy"

msgctxt "SettingsOptions::access_restrictions::unsuccessful_attempts_login"
msgid "Block IP after a number of unsuccessful attempts"
msgstr "Blokovať IP po niekoľkých neúspešných pokusoch"

msgctxt "SettingsOptions::access_restrictions::number_unsuccessful_attempts"
msgid "Number of unsuccessful attempts"
msgstr "Počet neúspešných pokusov"

msgctxt "SettingsOptions::access_restrictions::login_intervals"
msgid "Time between unsuccessful login attempts (seconds)"
msgstr "Čas medzi neúspešnými pokusmi na prihlásenie (v sekundách)"

msgctxt "SettingsOptions::access_restrictions::time_block"
msgid "Time for which the IP should be blocked (hours)"
msgstr "Čas, počas ktorého budú blokované IP adresy (hodiny)"

msgctxt "SettingsOptions::access_restrictions::elm_customer_and_affiliate_area_settings"
msgid "Customer area settings"
msgstr "Nastavenie zákazníckej časti"

msgctxt "SettingsOptions::access_restrictions::unsuccessful_attempts_login_customer"
msgid "Block IP after a number of unsuccessful attempts"
msgstr "Blokovať IP po niekoľkých neúspešných pokusoch"

msgctxt "SettingsOptions::access_restrictions::number_unsuccessful_attempts_customer"
msgid "Number of unsuccessful attempts"
msgstr "Počet neúspešných pokusov"

msgctxt "SettingsOptions::access_restrictions::login_intervals_customer"
msgid "Time between unsuccessful login attempts (seconds)"
msgstr "Čas medzi neúspešnými pokusmi na prihlásenie (v sekundách)"

msgctxt "SettingsOptions::access_restrictions::time_block_customer"
msgid "Time for which the IP should be blocked (hours)"
msgstr "Čas, počas ktorého budú blokované IP adresy (hodiny)"

