msgctxt "Addons::name::image_zoom"
msgid "Image zoom"
msgstr "Obrázkový zoom"

msgctxt "Addons::description::image_zoom"
msgid "Shows zoomed image on product image hover"
msgstr "Zobrazí zvětšený detail při najetí myši nad náhled obrázku produktu"

msgctxt "SettingsSections::image_zoom::general"
msgid "General"
msgstr "Obecné"

msgctxt "SettingsOptions::image_zoom::cz_opacity"
msgid "Opacity"
msgstr "Neprůhlednost"

msgctxt "SettingsOptions::image_zoom::cz_animation_time"
msgid "Animation time"
msgstr "Čas animace"

