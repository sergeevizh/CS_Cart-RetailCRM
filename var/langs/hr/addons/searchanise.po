msgctxt "Addons::name::searchanise"
msgid "Searchanise"
msgstr "Searchanise"

msgctxt "Addons::description::searchanise"
msgid "Turns on the <a target=\"_blank\" href=\"http://searchanise.com/\">Searchanise</a> fast search service"
msgstr "Uključuje <a target=\"_blank\" href=\"http://searchanise.com/\"> Searchanise </a> usluge brzo traženje"

msgctxt "Languages::searchanise"
msgid "Searchanise"
msgstr "Searchanise"

msgctxt "Languages::text_se_import_status_queued"
msgid "The product catalog is queued for syncing with Searchanise."
msgstr "Katalog proizvoda je na čekanju za sinkronizaciju s Searchanise."

msgctxt "Languages::text_se_re_indexation_required"
msgid "Searchanise: Re-indexation required (click the <strong>Force re-indexation</strong> button in the <a href=\"[link]\">Searchanise admin panel</a>)."
msgstr "Searchanise: Re-indeksacije potrebne (kliknite <strong> Prisilvno ponovno usklađivanje </strong> u <a href=\"[link]\"> Searchanise admin panel </a>)."

msgctxt "Languages::text_se_new_engine_store"
msgid "Searchanise: New search engine for [store] created. Catalog import started."
msgstr "Searchanise: Nova tražilica [store] stvorena. Počeo uvoz katalog."

msgctxt "Languages::text_se_seo_settings_notice"
msgid "If you have modified any SEO add-on settings, please <a href=\"[link]\">Re-indexation</a> product catalog with Searchanise to update product URLs."
msgstr "Ako ste promijenili bilo koju postavku SEO dodataka <a href=\"[link]\"> Ponovno indeksiranje </a> katalog proizvoda sa Searchanise za ažuriranje URL-ova proizvoda."

msgctxt "Languages::text_se_connect_error"
msgid "An error occurred during Searchanise registration. Please try later."
msgstr "Došlo je do pogreške prilikom registracije Searchanise. Molimo pokušajte kasnije."

msgctxt "Languages::text_se_import_status_processing"
msgid "The product catalog syncing is in progress."
msgstr "U tijeku je sinkronizacija katalog proizvoda."

msgctxt "Languages::text_se_import_status_done"
msgid "The product catalog was synced successfully with Searchanise."
msgstr "Katalog proizvoda je uspješno sinkronizirana s Searchanise."

msgctxt "Languages::text_se_import_status_none"
msgid "The product catalog is not synced with Searchanise yet. Please press \"Sync\" button to start."
msgstr "Katalog proizvoda još nije sinkroniziran s Searchanise. Kliknite \"Sinkroniziraj\" gumb za početak."

msgctxt "Languages::text_se_data_update"
msgid "Please note that the data on Searchanise server is updated with a 2-3 minute delay after you make changes in your store."
msgstr "Imajte na umu da se podaci na Searchanise poslužitelju ažuriraju s 2-3 minuta zakašnjenja nakon što napravite promjene u vašoj trgovini."

msgctxt "Languages::text_se_signup"
msgid "Click the button below to connect to Searchanise."
msgstr "Kliknite na gumb ispod za povezivanje s Searchanise."

msgctxt "Languages::text_se_just_connected"
msgid "Congratulations, you've just connected to Searchanise."
msgstr "Čestitamo, upravo ste povezali s Searchanise."

msgctxt "Languages::text_se_connected"
msgid "You've connected to Searchanise."
msgstr "Vi ste se povezali s Searchanise."

msgctxt "Languages::text_se_database_restore_notice"
msgid "You've just restored your data from the database dump file. If this dump contains product data, please <a href=\"[link]\">Re-indexation</a> it with Searchanise."
msgstr "Upravo ste vratili podatke iz dump datoteke baze podataka. Ako ovo datoteka sadrži podatke o proizvodu, molimo vas da <a href=\"[link]\"> Ponovno indeksiranje</a> s Searchanise."

msgctxt "Languages::text_se_only_root_can_register"
msgid "Only the root admin has the permission to connect to Searchanise"
msgstr "Samo root admin ima pravo spajanja na Searchanise"

