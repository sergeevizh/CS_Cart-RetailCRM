                    </item>
                    <item id="settings">
                        <type>header</type>
                    </item>
                    <item id="map_provider_settings">
                        <type>template</type>
                        <default_value>map_provider_settings.tpl</default_value>
                    </item>
                </items>
            </section>
        </sections>
    </settings>

    <queries>
    </queries>
</addon>
